CREATE TABLE paquetes (
    id SERIAL PRIMARY KEY, -- ID del paquete, tipo entero autoincremental
    numero_departamento VARCHAR(50) NOT NULL, -- Número de departamento, tipo cadena
    remitente VARCHAR(100) NOT NULL, -- Remitente, tipo cadena
    fecha_entrega DATE NOT NULL, -- Fecha de entrega, tipo fecha
    estado VARCHAR(50) NOT NULL, -- Estado del paquete, tipo cadena
    codigo_qr VARCHAR(100) UNIQUE, -- Código QR, tipo cadena, únicoo
    fecha_retiro DATE, -- Fecha de retiro, tipo fecha
    notas TEXT, -- Notas adicionales, tipo texto
    metodo_entrega VARCHAR(50), -- Método de entrega, tipo cadena
    fecha_registro TIMESTAMP DEFAULT CURRENT_TIMESTAMP -- Fecha de registro, tipo timestamp
);

--tabla a
